    "axios": "^0.21.1",
    "connected-react-router": "^6.9.1",
    "d3-array": "^3.0.1",
    "date-fns": "^2.21.3",
    "google-libphonenumber": "^3.2.21",
    "grommet": "^2.17.2",
    "grommet-icons": "^4.5.0",
    "grommet-styles": "^0.2.0",
    "history": "^4.10.1",
    "immutable": "^3.8.2",
    "luxon": "^1.27.0",
    "normalizr": "^3.6.1",
    "query-string": "^6.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-google-login": "^5.2.2",
    "react-phone-number-input": "^3.1.21",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
