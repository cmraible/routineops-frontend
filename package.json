    "@reduxjs/toolkit": "^1.5.0",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "d3-array": "^2.8.0",
    "date-fns": "^2.16.1",
    "google-libphonenumber": "^3.2.15",
    "grommet": "^2.16.0",
    "grommet-icons": "^4.5.0",
    "grommet-styles": "^0.2.0",
    "history": "^4.10.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "normalizr": "^3.6.1",
    "nyc": "^15.1.0",
    "query-string": "^6.13.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-google-login": "^5.1.25",
    "react-phone-number-input": "^3.1.6",
    "react-player": "^2.7.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rrule": "^2.6.6",
    "seamless-immutable": "^7.1.4",
    "source-map-explorer": "^2.5.1",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "coverage": "open coverage/lcov-report/index.html",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "npx cypress open",
    "eject": "react-scripts eject",
    "flushdb": "docker-compose -f ../routineops-backend/docker-compose.yaml exec -T web ./manage.py flush --no-input",
    "initdb": "docker-compose -f ../routineops-backend/docker-compose.yaml exec -T web ./manage.py initdb",
    "loaddata": "docker-compose -f ../routineops-backend/docker-compose.yaml exec -T web ./manage.py loaddata full",
    "resetdb": "docker-compose -f ../routineops-backend/docker-compose.yaml exec -T web ./manage.py flush --no-input && docker-compose -f ../routineops-backend/docker-compose.yaml exec -T web ./manage.py initdb"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.5",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^4.16.1",
    "cypress": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.2",
    "install": "^0.13.0",
    "istanbul-lib-coverage": "^3.0.0",
    "npm": "^6.14.9",
    "redux-devtools": "^3.7.0",
    "redux-devtools-extension": "^2.13.8"
  }
}
